	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"ios": "node node_modules/react-native/local-cli/cli.js start",
		"ios-bundle": "node node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
		"ios-dev-bundle": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
		"android-bundle": "node node_modules/react-native/local-cli/cli.js --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
		"android-dev-bundle": "node node_modules/react-native/local-cli/cli.js --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
		"web-bundle": "webpack --config web/webpack/web.prod.config.js --progress --colors",
		"web": "webpack-dev-server --content-base web/public/ --config web/webpack/web.dev.config.js --port 3000 --inline --hot --colors",
		"clear-cache": "gulp clear-cache",
		"test": "jest"
	},
	"dependencies": {
